{
  "name": "vue-gapi",
  "version": "0.3.1",
  "description": "Google API client wrapper for Vue",
  "author": "CedricPoilly <cedricpoilly@outlook.com>",
  "main": "dist/vue-gapi.common.js",
  "module": "dist/vue-gapi.esm.js",
  "browser": "dist/vue-gapi.common.js",
  "unpkg": "dist/vue-gapi.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "node build/build.js",
    "lint": "yon run lint:eslint && yon run lint:prettier",
    "lint:eslint": "eslint --ext js .",
    "lint:prettier": "prettier --check \"{**/*,*}.{js,json}\"",
    "lint:staged": "lint-staged",
    "test": "jest",
    "prepublish": "yon run build"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.{js,json}": [
      "prettier --write"
    ]
  },
  "pre-commit": "lint:staged",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-jest": "^26.1.0",
    "buble": "^0.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "mkdirp": "^0.5.1",
    "pre-commit": "^1.2.0",
    "prettier": "^2.0.5",
    "rimraf": "^2.6.0",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^1.1.0",
    "uglify-js": "^3.4.7",
    "uppercamelcase": "^3.0.0",
    "vue": "^2.5.17",
    "whatwg-fetch": "^3.0.0",
    "yarn-or-npm": "^2.0.0"
  },
  "peerDependencies": {
    "vue": "^2.3.0"
  },
  "homepage": "https://github.com/CedricPoilly/vue-gapi#readme",
  "bugs": {
    "url": "https://github.com/CedricPoilly/vue-gapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CedricPoilly/vue-gapi.git"
  },
  "license": "MIT",
  "dependencies": {}
}
